# waveshield SDK Environment Variables
# Copy this file to .env and fill in your actual values
# DO NOT commit the .env file to version control

# Security Configuration
# The SDK supports loading security credentials from environment variables
# with the prefix: WAVESHIELD_
#
# Security environment variables:
WAVESHIELD_API_KEY_AUTH=your_apikeyauth_here
WAVESHIELD_API_SECRET_AUTH=your_apisecretauth_here

# Debug Configuration
# Enable HTTP request/response logging for debugging
WAVESHIELD_DEBUG=true
